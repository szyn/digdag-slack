_export:
  plugin:
    repositories:
       - file://${repos}
    dependencies:
       - io.digdag.plugin:digdag-slack:${version}
  webhook_url: https://hooks.slack.com/services/${token}
  workflow_name: slack
  ENV: test

+success:
  slack>: templates/good.yml

